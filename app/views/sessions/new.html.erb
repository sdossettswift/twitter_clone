<div class="page-header">
  <h2>Sign In</h2>
</div>
  <div class="well">
  <%= form_tag do %>
    <div class="form-group">
      <%= label_tag :username %>
      <%= text_field_tag :username, params[:username], class: "form-control"%>
    </div>

    <div class="form-group">
      <%= label_tag :password%>
      <%= text_field_tag :password, "", class: "form-control"%>
    </div>

    <div class="form-group">
      <%= submit_tag "Sign In", class: "btn btn-lg btn-info"%>
    </div>
  <%end%>
  </div>
</div>
